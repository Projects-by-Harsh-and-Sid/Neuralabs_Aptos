# Create __init__.py files for all packages

# core/__init__.py
"""Core module for flow execution."""

# elements/__init__.py
"""Elements module containing all flow block types."""

# elements/flow_control/__init__.py
"""Flow control elements for managing execution paths."""

# elements/inputs/__init__.py
"""Input elements for providing data to flows."""

# elements/onchain/__init__.py
"""Blockchain interaction elements."""

# elements/util/__init__.py
"""Utility elements for data manipulation."""

# elements/ai/__init__.py
"""AI model integration elements."""

# elements/custom/__init__.py
"""Custom code execution elements."""

# services/__init__.py
"""Service integrations for external systems."""

# utils/__init__.py
"""Utility functions and helpers."""
